#include <stdio.h>
#include <stdlib.h>

int main (void)
{
	float priceOfGasInCalifornia= 1.88; 
	int y = 0;
	short a=1;
	double radius= 2.64567;
	y=12;
	
	printf("The price of californian gas is %f\n",priceOfGasInCalifornia);
	printf("y:%d\n",y);
	printf("a=%d\n",a);
	printf("radius=%lf\n",radius);
	
}
